{
  "name": "of-cli",
  "version": "0.0.1",
  "description": "OmniFocus CLI for AI agents",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "of": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "omnifocus",
    "cli",
    "productivity",
    "applescript"
  ],
  "author": "",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "commander": "^14.0.3"
  }
}
